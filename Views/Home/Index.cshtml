@{
    ViewBag.Title = "Log in";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!--==========================
    Intro Section
  ============================-->
<section id="intro">
    <div class="intro-container wow fadeIn">
        <h1 class="mb-4 pb-0">BLINK</h1>
        <h3> The <span>Learning Management System </span> of future</h3>
        <p class="mb-4 pb-0"></p>
        <ul class="nav-menu">
            <li class="buy-tickets">
                <a href="@Url.Action("Login", "Account", new { area = ""})" title="LogIn"><div><i class="fa fa-power-off fa-2x"></i></div></a>
            </li>
        </ul>
    </div>

</section>

<main id="main">

    <!--==========================
      Schedule Section
    ============================-->
    <section id="schedule" class="section-with-bg">
        <div class="container wow fadeInUp">
            <div class="section-header">
                <h2>LMS</h2>
                <p> Managing it All at Your Finger Tips</p>
            </div>

            <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" href="#day-1" role="tab" data-toggle="tab">Learning</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#day-2" role="tab" data-toggle="tab">Training</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#day-3" role="tab" data-toggle="tab">	Development </a>
                </li>
            </ul>



            <div class="tab-content row justify-content-center">

                <!-- Schdule Day 1 -->
                <div role="tabpanel" class="col-lg-9 tab-pane fade show active" id="day-1">
                    <h3 class="sub-heading">
                        An ongoing  process by which a person constructs their knowledge ,skills and capabilities .It's ongoing..
                        Let's make it Easy and accessible
                    </h3>
                </div>
                <!-- End Schdule Day 1 -->
                <!-- Schdule Day 2 -->
                <div role="tabpanel" class="col-lg-9  tab-pane fade" id="day-2">
                    <h3 class="sub-heading">
                        Enabling the Learning by most suited modes ..Instructor Led ,Experiential ,simulation,E- learning ,On the Job ..You Desire We Deliver
                    </h3>

                </div>
                <!-- End Schdule Day 2 -->
                <!-- Schdule Day 3 -->
                <div role="tabpanel" class="col-lg-9  tab-pane fade" id="day-3">
                    <h3 class="sub-heading">
                        Increasing the Progressive Capability for future
                    </h3>
                </div>
                <!-- End Schdule Day 2 -->
            </div>
        </div>
    </section>

    <!--==========================
      Sponsors Section
    ============================-->
    <section id="supporters" class="section-with-bg wow fadeInUp">

        <div class="container">
            <div class="section-header">
                <h2>Our Resources</h2>
            </div>
            <div class="row clearfix">
                <div class="col-lg-3 col-md-2 col-xs-2"></div>
                <div class="col-lg-8 col-md-8 col-xs-8">
                    <img src="~/img/Resources.png" class="img-fluid" alt="" />
                </div>
                <div class="col-lg-1 col-md-2 col-xs-2"></div>
            </div>
        </div>
    </section>



@using AJSolutions.Models;
@model IEnumerable<EndUserAttendedCount>
@{
    Layout = "~/Areas/CMS/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "DashBoard";
    var nominated = ViewData["Nominated"] as IEnumerable<AJSolutions.Models.EndUserNominatedCount>;
    var attended = ViewData["Attended"] as IEnumerable<AJSolutions.Models.EndUserAttendedCount>;
    var certified = ViewData["Certified"] as IEnumerable<AJSolutions.Models.EndUserCertifiedCount>;
}

<h2></h2>
@using (Html.BeginForm("EndUserDashBoard", "Course", FormMethod.Post, new { area = "TMS", @class = "form-horizontal needs-validation", role = "form" }))
{
    <div class="container">
        <div class="row">
            <div class="fancy-title title-dotted-border title-center">
                <h5><span>Training Type </span>Stats</h5>
            </div>
            <div class="col_full">
                <div class="table-responsive">
                    <table class="table table-bordered table-responsive nobottommargin small">
                        <thead>
                            <tr>
                                <th class="center">
                                    Training Type
                                </th>
                                <th class="center">
                                    Nominated
                                </th>
                                <th class="center">
                                    Attended
                                </th>
                                <th class="center">
                                    Certified
                                </th>

                            </tr>
                        </thead>
                        <tbody>

                            <tr>
                                <td class="info center">Finacle</td>
                                <td class="info center">@nominated.Select(a => a.TotalPATraineeNominated).Sum()</td>
                                <td class="info center">@attended.Select(a => a.TotalPATraineeAttended).Sum()</td>
                                <td class="info center">@certified.Select(a => a.TotalPACertified).Sum()</td>
                            </tr>
                            <tr>
                                <td class="warning center">Micro ATM</td>
                                <td class="warning center">@nominated.Select(m => m.TotalGDSTraineeNominated).Sum()</td>
                                <td class="warning center">@attended.Select(m => m.TotalGDSTraineeAttended).Sum()</td>
                                <td class="warning center">@certified.Select(m => m.TotalGDSCertified).Sum()</td>
                            </tr>
                            <tr>
                                <td class="success center">Total</td>
                                <td class="success center">@nominated.Select(a => a.TotalTraineeNominated).Sum()</td>
                                <td class="success center">@attended.Select(a => a.TotalTraineeAttended).Sum()</td>
                                <td class="success center">@certified.Select(a => a.TotalTraineeCertified).Sum()</td>

                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

            <div class="col_full">
                <div class="col_one_third button-white" id="polarAreaChartPA">
                    <strong class="center">Finacle</strong>
                    <canvas id="TotalFinaclePolar" height="300"></canvas>

                </div>
                <div class="col_one_third button-white" id="polarAreaChartGDS">
                    <strong class="center">Micro ATM</strong>
                    <canvas id="TotalGdsPolar" height="300"></canvas>

                </div>
                <div class="col_one_third button-white col_last" id="polarAreaChartTOT">
                    <strong class="center">Total</strong>
                    <canvas id="TotalPolar" height="300"></canvas>

                </div>
            </div>

           
            <div class="col_full">
                <div class="fancy-title title-dotted-border title-center">
                    <h5><span>Circle wise </span>Stats</h5>
                </div>
                <div class="table-responsive">
                    <table class="table table-bordered table-responsive nobottommargin small">
                        <thead>
                            <tr>
                                <th rowspan="2" class="center">Circle</th>

                                <th colspan="3" class="info center">Finacle</th>


                                <th colspan="3" class="warning center">Micro ATM</th>
                                <th class="success center" colspan="3">Total</th>

                            </tr>
                            <tr>

                                <th class="info center">Nominated</th>
                                <th class="info center">Attended</th>
                                <th class="info center">Certified</th>
                                <th class="warning center">Nominated</th>
                                <th class="warning center">Attended</th>
                                <th class="warning center">Certified</th>


                                <th class="success center">Nominated</th>
                                <th class="success center">Attended</th>
                                <th class="success center">Certified</th>


                            </tr>
                        
                        </thead>
                        <tbody>
                            @foreach (var item in nominated.OrderBy(m => m.Circle))
                                {
                                    var mitem = attended.Where(m => m.Circle == item.Circle).ToList();
                                    var citem = certified.Where(m => m.Circle == item.Circle).ToList();
                                    var totalnominatod = nominated.Where(m => m.Circle == item.Circle).ToList();

                                <tr>
                                    <td>@item.Circle</td>
                                    <td class="info center">@item.TotalPATraineeNominated</td>
                                    @foreach (var nitem in mitem)
                                        {
                                    <td class="info center">@nitem.TotalPATraineeAttended</td>
                                        }
                                    @foreach (var ditem in citem)
                                        {
                                    <td class="info center">@ditem.TotalPACertified</td>
                                        }
                                    <td class="warning center">@item.TotalGDSTraineeNominated</td>
                                    @foreach (var nitem in mitem)
                                        {
                                    <td class="warning center">@nitem.TotalGDSTraineeAttended</td>
                                        }
                                    @foreach (var ditem in citem)
                                        {
                                    <td class="warning center">@ditem.TotalGDSCertified</td>
                                        }
                                    @foreach (var tnominated in totalnominatod)
                                        {
                                        <td class="success center">@tnominated.TotalTraineeNominated</td>
                                        }
                                    @foreach (var tattended in mitem)
                                        {
                                        <td class="success center">@tattended.TotalTraineeAttended</td>
                                        }
                                    @foreach (var tcertified in citem)
                                        {
                                        <td class="success center">@tcertified.TotalTraineeCertified</td>
                                        }

                                </tr>

                                }
                            <tr>
                                <td>Total</td>
                                <td class="center">@nominated.Select(a => a.TotalPATraineeNominated).Sum()</td>
                                <td class="center">@attended.Select(a => a.TotalPATraineeAttended).Sum()</td>
                                <td class="center">@certified.Select(a => a.TotalPACertified).Sum()</td>
                                <td class="center">@nominated.Select(m => m.TotalGDSTraineeNominated).Sum()</td>
                                <td class="center">@attended.Select(m => m.TotalGDSTraineeAttended).Sum()</td>
                                <td class="center">@certified.Select(m => m.TotalGDSCertified).Sum()</td>
                                <td class="center">@nominated.Select(a => a.TotalTraineeNominated).Sum()</td>
                                <td class="center">@attended.Select(a => a.TotalTraineeAttended).Sum()</td>
                                <td class="center">@certified.Select(a => a.TotalTraineeCertified).Sum()</td>
                                </tr>
                        </tbody>
                    </table>
                </div>


                <script type="text/javascript">

                    jQuery(window).load(function () {
                        var labels = [], datapoint1 = [], datapoint2 = [], datapoint3 = [];
                        $.ajax({
                            type: "post",
                            url: "/TMS/Course/GetMultiTrendTotal/",
                            data: {

                            },
                            datatype: "json",
                            traditional: true,
                            success: function (data) {
                                for (var i = 0; i < data.length ; i++) {
                                    labels.push(data[i].Circle);
                                    datapoint1.push(data[i].TotalTraineeNominated);
                                    datapoint2.push(data[i].TotalTraineeAttended);
                                    datapoint3.push(data[i].TotalTraineeCertified);
                                }
                                 
                                var lineChartData = {
                                    labels: labels,
                                    datasets: [
                                        {
                                            label: "Nominated",
                                            showInLegend: true,
                                            name: "Nominated",
                                            fillColor: "rgba(217,112,65,0.5)",
                                            strokeColor: "rgb(217,112,65)",
                                            pointColor: "rgb(217,112,65)",
                                            pointStrokeColor: "#fff",
                                            data: datapoint1,
                                            indexlabel: "Nominated"
                                        },
                                        {
                                            label: "Attended",
                                            showInLegend: true,
                                            text: "Attended",
                                            fillColor: "rgba(127,255,212,0.5)",
                                            strokeColor: "rgb(127,255,212)",
                                            pointColor: "rgb(127,255,212)",
                                            pointStrokeColor: "#fff",
                                            data: datapoint2
                                        },
                                        {
                                            label: "Certified",
                                            showInLegend: true,
                                            name: "Certified",
                                            fillColor: "rgba(33,50,61,0.5)",
                                            strokeColor: "rgb(33,50,61)",
                                            pointColor: "rgb(33,50,61)",
                                            pointStrokeColor: "#fff",
                                            data: datapoint3
                                        }
                                    ]
                                }


                                var globalGraphSettings = { animation: Modernizr.canvas };

                                function showLineChart() {
                                    var ctx = document.getElementById("lineChartCanvas").getContext("2d");
                                    new Chart(ctx).Line(lineChartData, globalGraphSettings);
                                }

                                $('#lineChart').appear(function () { $(this).css({ opacity: 1 }); setTimeout(showLineChart, 300); }, { accX: 0, accY: -155 }, 'easeInCubic');
                            }
                        })



                    });

                </script>



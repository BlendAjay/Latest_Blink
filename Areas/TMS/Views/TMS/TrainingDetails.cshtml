<script type="text/javascript">
    function printpage() {


        //Get the print button and put it into a variable
        var printButton = document.getElementById("printpagebutton");
        //Set the print button visibility to 'hidden'
        printButton.style.visibility = 'hidden';

        //  var w = window.open(window.location.href, "_blank")



        //Print the page content
        window.print();

        //Set the print button to 'visible' again
        //[Delete this line if you want it to stay hidden after printing]
        // printButton.style.visibility = 'visible';
        return false;
    }

    function attach(TrainingId) {
        var paramPub;
        paramPub = "TrainingId=" + TrainingId;
        paramPub = $('#weburl').val() + "/TMS/User/ConfirmEmployee?" + paramPub;
        $('.form-process').fadeIn();
        $('#cndBody').load(paramPub);
        $("#cutoff").modal({
            backdrop: 'static',
            keyboard: false,
        })
    };

    function Final() {
        $('#cndBody').load();
        $("#cutoff").modal({
            backdrop: 'static',
            keyboard: false,
        })
    };

    $(document).ready(function () {
        if ($("#SaveStatus").val() == "True" || $("#SaveStatus").val() == "Succeeded") {
            $.alert({
                title: 'Cheers',
                content: 'Training Updated <b> SUCCESSFULLY.</b>',
                confirmButton: 'OK',
                confirmButtonClass: 'btn btn-primary',
                icon: 'fa fa-thumbs-up fa-2x',
                animation: 'zoom',
                confirm: function () {
                    var url = $("#RedirectToHome").val();
                    location.href = url;
                }
            });
        }
    });

    function TrainingSummReport(BatchId, TrainingId) {
        var url = $("#RedirectToReport").val() + "?BatchId=" + BatchId + "&TrainingId=" + TrainingId;
        window.location.href = url;
    }

    function pagehelp() {
        $('#Popup').modal('show');
        $("#Popup").modal({
            backdrop: 'static',
            keyboard: false,
        })
    };


    function Notifications(TrainingId, CourseCode) {
        $('.form-process').fadeIn()
        $.ajax({
            type: "post",
            url: "/TMS/TMS/SendNotification",
            data: {
                TrainingId: TrainingId,
                CourseCode: CourseCode
            },
            datatype: "json",
            traditional: true,
            success: function (data) {
                $('.form-process').fadeOut()
                if (data != null) {
                    $.alert({
                        title: 'Cheers',
                        content: 'Notifications Sent Successfully',
                        confirmButton: 'OK',
                        confirmButtonClass: 'btn btn-primary',
                        icon: 'fa fa-thumbs-down fa-2x ',
                        animation: 'zoom',
                        confirm: function () {
                            var url = $("#RedirectToHome").val() + "?Id=" + TrainingId;
                            location.href = url;
                        }
                    });
                }
            }
        });
    }

    function AssessmentNotifications(AssessmentId, TrainingId) {
        $('.form-process').fadeIn()
        $.ajax({
            type: "post",
            url: "/TMS/TMS/SendAssessmentNotification",
            data: {
                TrainingId: TrainingId,
                AssessmentId: AssessmentId,
                CourseCode: CourseCode
            },
            datatype: "json",
            traditional: true,
            success: function (data) {
                $('.form-process').fadeOut()
                if (data != null) {
                    $.alert({
                        title: 'Cheers',
                        content: 'Assessment Notifications Sent Successfully',
                        confirmButton: 'OK',
                        confirmButtonClass: 'btn btn-primary',
                        icon: 'fa fa-thumbs-down fa-2x ',
                        animation: 'zoom',
                        confirm: function () {
                            var url = $("#RedirectToHome").val() + "?Id=" + TrainingId;
                            location.href = url;
                        }
                    });
                }
            }
        });
    }
</script>

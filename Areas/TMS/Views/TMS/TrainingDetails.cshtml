@using AJSolutions.DAL
@using AJSolutions.Models;
@model AJSolutions.Models.TrainingScheduleView
@{
    ViewBag.Title = "Training Details";
    AJSolutions.Models.UserViewModel user = ViewData["UserProfile"] as AJSolutions.Models.UserViewModel;
    if (user.Role != "Employee")
    {
        Layout = "~/Areas/CMS/Views/Shared/_Layout.cshtml";
    }
    else
    {
        Layout = "~/Areas/EMS/Views/Shared/_Layout.cshtml";
    }
    var userId = ViewBag.UserId as string;
    List<AJSolutions.Models.TrainingScheduleAttachment> attachments = ViewData["Attachment"] as List<AJSolutions.Models.TrainingScheduleAttachment>;
    List<AJSolutions.Models.TrainingScheduleFinalAttachment> finalattachments = ViewData["FinalAttach"] as List<AJSolutions.Models.TrainingScheduleFinalAttachment>;
    CourseBatchViewModel batch = ViewData["CourseBatches"] as CourseBatchViewModel;
    List<TrainingAssessmentView> assessment = ViewData["TrainingAssessment"] as List<TrainingAssessmentView>;
    int sno = 0;
}
<div class="si-sticky toast-top-left topmargin-sm">
    <a href="#" class="social-icon si-rounded si-colored si-aim si-small" onclick="pagehelp();" title="Help For You">
        <i class="icon-question"></i>
        <i class="icon-question"></i>
    </a>
</div>

@using (Html.BeginForm())
{
    @Html.Hidden("SaveStatus", ViewBag.Result as string)
    if (Model != null)
    {
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="page-header" id="top">
                    <h2 class="pageheader-title">Training Details </h2>
                    <div class="page-breadcrumb">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">Dashboard</a></li>
                                <li class="breadcrumb-item"><a href="#" class="breadcrumb-link">My Training</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Training Details</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="page-section" id="overview">
                        <div class="row">
                            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                <div class="row">
                                    <div class="col-xl-7 col-lg-12 col-md-12 col-sm-12 col-12">
                                        <h2>@Model.SubjectLine</h2>
                                    </div>
                                    <div class="col-xl-2 col-lg-12 col-md-12 col-sm-12 col-12">
                                        @if (user.DepartmentId == "ADI")
                                        {
                                            <a href="#" id="Notifications" class="badge badge-info" onclick="javascript: Notifications('@Model.TrainingId','@Model.CourseCode');" title="Send Notification">Send Notification</a>
                                        }
                                    </div>
                                    <div class="col-xl-3 col-lg-12 col-md-12 col-sm-12 col-12">
                                        <div class="float-xl-right float-none mt-xl-0 mt-4">

                                            @if (user.Role != "Client")
                                            {
                                                <div class="col_full">
                                                    @if (Model.Status == "Assigned" && userId == Model.TrainerId)
                                                    {
                                                        <button type="button" class="btn btn-warning  btn-sm" onclick="location.href='@Url.Action("TrainingDetails", "TMS", new { area = "TMS", Id = Model.TrainingId, Status = "Inprogress" })'">Accept</button>
                                                    }
                                                    else if (Model.Status == "Inprogress" && userId == Model.TrainerId)
                                                    {
                                                        <button type="button" class="btn btn-warning  btn-sm" onclick="Final('@Model.TrainingId');">Complete</button>
                                                    }
                                                    else if (Model.Status == "Assigned" && userId == Model.TrainerId)
                                                    {
                                                        <button type="button" class="btn btn-warning  btn-sm" style="float:right;" onclick="location.href='@Url.Action("TrainingDetails", "TMS", new { area = "TMS", Id = Model.TrainingId, Status = "Canceled" })'">Cancel Training</button>
                                                    }
                                                    @if (Model.Status == "Assigned" && userId == Model.TrainerId)
                                                    {
                                                        <button type="button" class="btn btn-primary btn-sm" onclick="location.href='@Url.Action("MyTraining", "TMS", new { area = "TMS", Id = Model.TrainingId, Status = "Assigned" })'">Reject</button>
                                                    }
                                                    @if (user.DepartmentId == "FAC" && Model.Status == "Inprogress" && (user.UserId != Model.TrainerMentorId || Model.TrainerMentorId == Model.TrainerId))//
                                                    {
                                                        <a href="@Url.Action("Add", "User", new { Area = "CMS", Id = "CND", Batch = Model.BatchId, CId = batch.CorporateId })" target="_blank" class="button button-3d button-mini button-rounded button-teal" style="float:right;">Add Candidate</a>
                                                    }
                                                    @if (Model.Status == "Inprogress" && (user.UserId == Model.TrainerId || user.UserId == user.SubscriberId || user.UserId == Model.TrainerMentorId) && Model.AttendenceNeeded == true)// || user.UserId == Model.TrainerMentorId
                                                    {
                                                        <button type="button" class="btn btn-primary right  btn-sm" onclick="location.href='@Url.Action("Record", "Attendance" , new { area = "CMS", Id = Model.TrainingId})'">Attendance</button>
                                                    }
                                                    @if (Model.Status == "Inprogress" && (user.DepartmentId == "ADI" || user.UserId == user.SubscriberId || user.UserId == Model.TrainerMentorId) && assessment.Count > 0)
                                                    {
                                                        <a href="@Url.Action("TrainingEvaluation", "TMS", new { area = "TMS", Id = Model.TrainingId})" target="_blank" class="btn btn-primary" style="float:right;">Evaluation</a>
                                                    }
                                                </div>
                                            }

                                        </div>
                                    </div>
                                </div>
                                <p class="lead">@Model.Description</p>

                                <ul class="list-unstyled arrow">
                                    <li><a href="#"><i class="icon-road"></i>Address:  @Model.Address</a></li>
                                    <li><a href="#"><i class="icon-location"></i>City:  @Model.City</a></li>
                                    <li><a href="#"><i class="icon-location"></i>State:  @Model.State</a></li>
                                    <li><a href="#"><i class="icon-location"></i>Country:  @Model.Country</a></li>
                                    @if (Model.BatchName != null)
                                    {
                                        <li>
                                            <i class="icon-book"> </i> Course Name:
                                            <span class="badge">@Model.CourseName</span>
                                        </li>
                                    }
                                    @if (Model.BatchName != null)
                                    {
                                        <li>
                                            <i class="icon-stack"> </i> Batch Name:
                                            <span class="badge">@Model.BatchName</span>

                                        </li>
                                    }
                                    <li>
                                        <i class="icon-calendar"> </i> Training Schedule:
                                        <span class="badge">@batch.FromDate.ToString("dd-MMM-yyyy") &nbsp;&nbsp; - &nbsp;&nbsp;@batch.ToDate.ToString("dd-MMM-yyyy")</span>

                                    </li>
                                    @if (batch.FromTime != null || batch.ToTime != null)
                                    {
                                        <li>
                                            <i class="icon-clock"> </i> Schedule Time:
                                            <span class="badge">@batch.FromTime.TimeOfDay &nbsp;&nbsp; - &nbsp;&nbsp;@batch.ToTime.TimeOfDay</span>

                                        </li>
                                    }
                                    @if (Model.OtherTrainerId != null)
                                    {
                                        <li>
                                            <i class="icon-users"> </i> Other Trainer:
                                            <span class="badge">@Model.TrainerName</span>

                                        </li>
                                    }
                                    @if (Model.OtherTrainerId != null)
                                    {
                                        <li>
                                            <i class="icon-users"> </i> Total Trainer:
                                            <span class="badge">@(@Model.TrainersCount + 1)</span>

                                        </li>
                                    }
                                    @if (batch.ContentLink != null)
                                    {
                                        <li>
                                            <i class="icon-users"> </i> Course Content Link:
                                            <span class="badge"><a href="@batch.ContentLink" target="_blank" title="Click Here to Open Content Link">Content Link</a></span>

                                        </li>
                                    }
                                </ul>
                                <div>
                                    @if (assessment.Count() > 0)
                                    {
                                        <div class="list-group">
                                            <a href="#" class="list-group-item active">
                                                Training Evaluation
                                            </a>
                                            @foreach (var item in assessment)
                                            {
                                                <div class="list-group-item">
                                                    @{ sno++; }
                                                    @sno.  @item.Assessment (@item.Weightage %)
                                                    @if (Model.Status == "Inprogress" && (user.UserId == Model.TrainerId || user.UserId == Model.TrainerMentorId) && assessment.Count > 0)
                                                    {
                                                        <a href="@Url.Action("TrainingEvaluation", "TMS", new { area = "TMS", Id = Model.TrainingId, Assessment = item.AssessmentId })" target="_blank" class="btn btn-primary btn-xs" style="float:right;">Evaluation</a>
                                                    }
                                                    @if (user.DepartmentId == "ADI" && Model.Status == "Assigned")
                                                    {
                                                        <a href="#" id="AssessmentNotifications" class="btn btn-default btn-xs fright" onclick="javascript: AssessmentNotifications('@item.AssessmentId', '@Model.TrainingId');" title="Send Notification">Send Notification</a>
                                                    }
                                                </div>

                                            }
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>













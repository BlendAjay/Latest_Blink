@model AJSolutions.Models.JobOrderTypeMaster

@{
    ViewBag.Title = "Delete";
    AJSolutions.Models.UserViewModel user = ViewData["UserProfile"] as AJSolutions.Models.UserViewModel;
    if (user.Role != "Employee")
    {
        Layout = "~/Areas/CMS/Views/Shared/_Layout.cshtml";
    }
    else
    {
        Layout = "~/Areas/EMS/Views/Shared/_Layout.cshtml";
    }
}
<div class="container">
    <div id="contact-form-overlay" class="clearfix">
       <div class="row">
            <div class="fancy-title title-bottom-border">
                <h3>Are you sure you want to delete this?</h3>
            </div>
            <div class="col_half">
                @Html.DisplayNameFor(model => model.JobOrderType) : 
                <b> @Html.DisplayFor(model => model.JobOrderType)</b>
            </div>
            <div class="col_half col_last">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    <input type="submit" value="Delete" class="button button-3d button-rounded button-teal" style="float:right" />
                }
            </div>
        </div>
        <p>          
            @Html.ActionLink("Back to List", "Index")
        </p>
    </div>
</div>
